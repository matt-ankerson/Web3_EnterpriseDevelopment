@model List<ankem1IntroToEF.Models.Farmer>

@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Show Farmers</title>
</head>
<body>
    <div>
        <h3>Here are all the farmers.</h3>
        <table>
            @foreach (var farmer in @Model)
            {
                <tr>
                    <td>@farmer.FirstName</td>
                    <td>@farmer.LastName</td>
                    <td>@farmer.Phone</td>
                </tr>
            }
        </table>
    </div>
</body>
</html>
